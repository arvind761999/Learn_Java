import java.util.Scanner;
class FibonacciSr
{
	public static void main(String[] args)
	{
	    Scanner sc = new Scanner(System.in);
	    int n = sc.nextInt();
	    int a = 0;
	    int b = 1;

        System.out.println(a+" "+ b);
	    Fibonacci(a,b,n)
	}

	public static void main(int a, int b, int n){

	       // Base case
	       if(n == 0){
              return;
	       }

           int c = a+b;
           System.out.println(c+" ");

           //recursive call
           Fibonacci(b, c, n--);
	}
}